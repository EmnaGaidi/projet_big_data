{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Stream",
      "request": "launch",
      "mainClass": "hadoop.streaming.Stream",
      "projectName": "streamnumber"
    },
    {
      "type": "java",
      "name": "ConsumerMongo",
      "request": "launch",
      "mainClass": "kafkastream.ConsumerMongo",
      "projectName": "streamnumber"
    },
    {
      "type": "java",
      "name": "ProducerKafka",
      "request": "launch",
      "mainClass": "kafkastream.ProducerKafka",
      "projectName": "streamnumber",
      "args": ["topic1"]
    },
    {
      "type": "java",
      "name": "StreamMongoDB",
      "request": "launch",
      "mainClass": "hadoop.streaming.StreamMongoDB",
      "projectName": "streamnumber"
    },
    {
      "type": "java",
      "name": "StreamsByArtistMongoDB",
      "request": "launch",
      "mainClass": "hadoop.mapreduce.batchSpark.StreamsByArtistMongoDB",
      "projectName": "streamnumber",
      "args": [
        "streamnumber/src/main/resources/input/Popular_Spotify_Songs.csv",
        "streamnumber/src/main/resources/output1"
      ]
    },
    {
      "type": "java",
      "name": "ConsumerSpark",
      "request": "launch",
      "mainClass": "kafkastream.ConsumerSpark",
      "projectName": "streamnumber",
      "args": ["localhost:9092", "topic1", "mySparkConsumerGroup"]
    },
    {
      "type": "java",
      "name": "Stream2",
      "request": "launch",
      "mainClass": "hadoop.stream2.Stream2",
      "projectName": "streamnumber"
    },
    {
      "type": "java",
      "name": "Current File",
      "request": "launch",
      "mainClass": "${file}"
    },
    {
      "type": "java",
      "name": "StreamsByArtistMain",
      "request": "launch",
      "mainClass": "hadoop.mapreduce.StreamsByArtistMain",
      "projectName": "streamnumber",
      "args": [
        "streamnumber/src/main/resources/input/Popular_Spotify_Songs.csv",
        "streamnumber/src/main/resources/output1"
      ]
    }
  ]
}
